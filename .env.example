# TLXAUTO - exemplo de variáveis (NÃO commitar .env real)

# Backend
JWT_SECRET=CHANGE_ME_SUPER_SECRET
PWD_SALT=CHANGE_ME_PEPPER
ADMIN_KEY=CHANGE_ME_ADMIN_KEY
JWT_EXPIRES_MIN=240
DEV_RETURN_TOKEN_LINKS=1

# URLs públicas (ajuste em produção)
PUBLIC_WEB_BASE=http://localhost:5173
PUBLIC_API_BASE=http://localhost:8000

# Pixel/CORS
PIXEL_ALLOWED_ORIGINS=http://localhost:5173

# Redis (rate limit)
REDIS_URL=redis://redis:6379/0
RATE_LIMIT_PER_MIN=120

# OpenTelemetry
OTEL_EXPORTER_OTLP_ENDPOINT=http://otel-collector:4318/v1/traces
OTEL_SERVICE_NAME=tlxauto-ads

# Postgres (upgrade futuro / opcional)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=tlx_ads
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/tlx_ads
